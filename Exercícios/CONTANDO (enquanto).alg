algoritmo "Contando"
var
    contador, salto, NFinal:inteiro
inicio
   escreval ("Escreva um número ")
   leia (contador)
   escreval ("Qual o último número da contagem? ")
   leia (NFinal)
   escreval ("Qual o salto?")
   leia (salto)
   enquanto (contador = NFinal)  faca
   escreval (contador)
   contador <- contador + salto
   fimenquanto
   

fimalgoritmo