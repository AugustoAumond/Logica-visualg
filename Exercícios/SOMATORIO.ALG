algoritmo "Somatorio"
var
      cont,N, S: inteiro
inicio
      cont <-0
      enquanto   (cont <=4) faca
      escreva ("Esse é o ", cont, "o . Valor: ")
      leia (N)
      S <- S + N
      cont <- cont + 1
      fimenquanto

      Escreval ( "A soma dos números foi de ", S)
fimalgoritmo