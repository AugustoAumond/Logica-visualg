algoritmo "semnome"
var
   X, Y: inteiro
   procedimento  Soma (A, B: inteiro)
   inicio
        Escreval ("Recebi valor ", A)
        Escreval ("Recebi o valor ", B)
        Escreval ("A soma entre ", A, " e ", B, " é ", A + B)
       fimprocedimento
inicio
         X <- 5
         Y <- 8
         Soma (X, Y)
fimalgoritmo